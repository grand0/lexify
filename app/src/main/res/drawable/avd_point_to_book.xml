<animated-vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">
    <aapt:attr name="android:drawable">
        <vector
            android:name="vector"
            android:width="24dp"
            android:height="24dp"
            android:viewportWidth="24"
            android:viewportHeight="24">
            <path
                android:name="path_1"
                android:pathData="M 12 10 L 12 10 C 10.9 10 10 10.9 10 12 L 10 12 C 10 13.1 10.9 14 12 14 L 12 14 C 13.1 14 14 13.1 14 12 L 14 12 C 14 10.9 13.1 10 12 10 Z M 12 12 L 12 12 L 12 12 L 12 12 L 12 12 Z"
                android:fillColor="#000000"
                android:fillType="evenOdd"/>
            <clip-path
                android:name="mask"
                android:pathData="M 6 4 L 14 4 L 14 14 L 6 14 Z"/>
            <group
                android:name="group"
                android:translateY="-9">
                <path
                    android:name="path_2"
                    android:pathData="M 9 4 L 11 4 L 11 9 L 10 8.25 L 9 9 L 9 4 Z M 7 4 L 7 13 L 10 10.75 L 13 13 L 13 4 L 7 4 Z"
                    android:fillColor="#000000"/>
            </group>
        </vector>
    </aapt:attr>
    <target android:name="path_1">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="pathData"
                android:duration="650"
                android:valueFrom="M 12 10 L 12 10 C 10.9 10 10 10.9 10 12 L 10 12 C 10 13.1 10.9 14 12 14 L 12 14 C 13.1 14 14 13.1 14 12 L 14 12 C 14 10.9 13.1 10 12 10 Z M 12 12 L 12 12 L 12 12 L 12 12 L 12 12 Z"
                android:valueTo="M 18 2 L 6 2 C 4.9 2 4 2.9 4 4 L 4 20 C 4 21.1 4.9 22 6 22 L 18 22 C 19.1 22 20 21.1 20 20 L 20 4 C 20 2.9 19.1 2 18 2 Z M 18 4 L 6 4 L 6 20 L 18 20 L 18 4 Z"
                android:valueType="pathType"
                android:interpolator="@android:interpolator/fast_out_slow_in"/>
        </aapt:attr>
    </target>
    <target android:name="group">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="translateY"
                android:startOffset="550"
                android:duration="450"
                android:valueFrom="-9"
                android:valueTo="0"
                android:valueType="floatType"
                android:interpolator="@android:interpolator/fast_out_slow_in"/>
        </aapt:attr>
    </target>
</animated-vector>
